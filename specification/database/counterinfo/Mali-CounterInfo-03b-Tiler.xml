    <StableID>336</StableID>
    <HumanName>Primitive assembly position shading stall cycles</HumanName>
    <GroupName>Tiler Stall Cycles</GroupName>
    <GroupHumanName>Primitive assembly position shading stalls</GroupHumanName>
    <Units>cycles</Units>
